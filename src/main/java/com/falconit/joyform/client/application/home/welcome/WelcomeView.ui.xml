<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:m="urn:import:gwt.material.design.client.ui"
             xmlns:ma="urn:import:gwt.material.design.addins.client"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:combobox="urn:import:gwt.material.design.addins.client.combobox"
             xmlns:m.table="urn:import:gwt.material.design.client.ui.table">
    <m:MaterialRow>

        <m:MaterialRow grid="l12 m12 s12">
            
            <m:MaterialRow grid="l12 m12 s12" textAlign="CENTER" marginTop="20">
                <ma:webp.MaterialWebpImage ui:field="photo" grid="l2 m2 s4" offset="l5 m5 m4" fallbackExtension="png" maxWidth="150" maxHeight="150" url="https://p7.hiclipart.com/preview/535/466/472/google-account-microsoft-account-login-email-gmail-email.jpg" type="CIRCLE" />
               </m:MaterialRow>
            <m:MaterialRow grid="l12 m12 s12" textAlign="CENTER">
                <m:MaterialTitle marginBottom="20" ui:field="txtname" title="Welcome User" description="Manage your info, privacy, and security to make falconIT work better for you" />
            </m:MaterialRow>
            
            <m:MaterialColumn grid="l6 m6 s12">
            <m:MaterialCard backgroundColor="GREY_LIGHTEN_3" shadow="0">
             <m:MaterialCardContent>
                <m:MaterialRow marginBottom="0">   
                    <m:MaterialIcon iconType="ACCOUNT_BOX" iconSize="LARGE" textColor="GREY"  float="LEFT" marginRight="20"/>
                    
                    <!--
                    <ma:webp.MaterialWebpImage grid="l4 m4 s4" fallbackExtension="png" url="http://cdn.onlinewebfonts.com/svg/img_208869.png" maxWidth="150" maxHeight="150" float="LEFT" marginRight="20"/>
                    
                    <ma:webp.MaterialWebpImage grid="l4 m4 s4" fallbackExtension="png" url="https://cdn3.iconfinder.com/data/icons/flat-rounded-7/50/617-512.png" maxWidth="150" maxHeight="150" float="LEFT" marginRight="20"/>
                    -->
                    <m:MaterialCardTitle text="Personal information" />
                    
                    <m:MaterialLabel text="See the data in your falconIT Account and saved" />
                <!-- See the data in your falconIT Account and choose what activity is saved to personalize your falconIT experience -->
                </m:MaterialRow>
             </m:MaterialCardContent>
             <m:MaterialCardAction>
              <m:MaterialLink href="#personal" text="Manage your data and personalization" iconType="CREATE" textColor="TEAL"/>
             </m:MaterialCardAction>
            </m:MaterialCard>
           </m:MaterialColumn>
           
                       
           <m:MaterialColumn grid="l6 m6 s12">
            <m:MaterialCard backgroundColor="GREY_LIGHTEN_3" shadow="0">
             <m:MaterialCardContent>
                <m:MaterialRow marginBottom="0">
                    <m:MaterialIcon iconType="APPS" iconSize="LARGE" textColor="GREY"  float="LEFT" marginRight="20" />
                    <!--
                    <ma:webp.MaterialWebpImage grid="l4 m4 s4" fallbackExtension="png" url="https://icon-library.net/images/apps-icon-png/apps-icon-png-20.jpg" maxWidth="150" maxHeight="150" float="LEFT" marginRight="20"/>
                    -->
                    <m:MaterialCardTitle text="Apps center" />
                    <m:MaterialLabel text="Single ID form service and workflow management" />
                </m:MaterialRow>
             </m:MaterialCardContent>
             <m:MaterialCardAction>
              <m:MaterialLink href="#dashboard" text="Manage and configure" textColor="TEAL" iconType="CREATE"/>
             </m:MaterialCardAction>
            </m:MaterialCard>
           </m:MaterialColumn>
           
            <m:MaterialColumn grid="l6 m6 s12">
            <m:MaterialCard backgroundColor="GREY_LIGHTEN_3" shadow="0">
             <m:MaterialCardContent>
                <m:MaterialRow marginBottom="0">
                    <m:MaterialIcon iconType="SECURITY" iconSize="LARGE" textColor="GREY"  float="LEFT" marginRight="20"/>
                    <!--
                    <ma:webp.MaterialWebpImage marginBottom="20" grid="l4 m4 s4" fallbackExtension="png" url="https://image21.net/103/security_png/security_png_16.png" maxWidth="150" maxHeight="150" float="LEFT" marginRight="20"/>
                    -->
                    <m:MaterialCardTitle text="Security" />
                    <m:MaterialLabel text="Protect your account, falconIT care your login devices, 2FA" />
                </m:MaterialRow>
             </m:MaterialCardContent>
             <m:MaterialCardAction>
              <m:MaterialLink text="Secure account" iconType="CREATE" textColor="TEAL"/>
             </m:MaterialCardAction>
            </m:MaterialCard>
           </m:MaterialColumn>

           
            <m:MaterialColumn grid="l6 m6 s12">
            <m:MaterialCard backgroundColor="GREY_LIGHTEN_3" shadow="0">
             <m:MaterialCardContent>
                <m:MaterialRow marginBottom="0">
                    
                    <m:MaterialIcon iconType="BEENHERE" iconSize="LARGE" textColor="GREY"  float="LEFT" marginRight="20"/>
                    <!--
                    <ma:webp.MaterialWebpImage grid="l4 m4 s4" fallbackExtension="png" url="https://www.educationalmeasures.com/hs-fs/hubfs/04-user-privacy.png?width=500&amp;name=04-user-privacy.png" maxWidth="150" maxHeight="150" float="LEFT" marginRight="20"/>
                    -->
                    <m:MaterialCardTitle text="Privacy" />
                    <m:MaterialLabel text="This step-by-step guide helps you choose the privacy settings that are right for you" />
                </m:MaterialRow>
             </m:MaterialCardContent>
             <m:MaterialCardAction>
              <m:MaterialLink text="Getting start" iconType="CREATE" textColor="TEAL"/>
             </m:MaterialCardAction>
            </m:MaterialCard>
           </m:MaterialColumn>

        </m:MaterialRow>

    </m:MaterialRow>
</ui:UiBinder>
